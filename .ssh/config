Host *
    ServerAliveInterval 30
    ControlMaster auto
    ControlPath /tmp/ssh_%n_%r

Host github.com
    HostName github.com
    User lvijay
    IdentityFile ~/.ssh/github.com_id_rsa

Host gitlab.com
    HostName gitlab.com
    User lvijay
    IdentityFile ~/.ssh/github.com_id_rsa

Host bitbucket.org
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/bitbucket_rsa
